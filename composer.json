{
    "name": "netflex/http",
    "description": "Netflex HTTP library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^12.0",
        "php": "^8.2"
    },
    "require-dev": {
        "illuminate/container": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Http\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Http\\Providers\\HttpClientServiceProvider"
            ],
            "aliases": {
                "Http": "Netflex\\Http\\Facades\\Http"
            }
        }
    }
}
